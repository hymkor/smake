(let ((EXE (q "go" "env" "GOEXE")))
  (format (standard-output) "EXE=~a~%" EXE)
  (make
    ((update "smake$(EXE)" "main.go")
     (x "go" "fmt")
     (x "go" "build")
     )
    ((update "update")
     (x "touch" "main.go")
     )
    ((update "README.md" "_README.md")
     (1> "README.md"
       (x "gmnlpp" "_README.md")
       )
     )
    )
  )
